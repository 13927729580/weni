.tabbar {
  position: fixed;
  bottom: 0;
  left: 0;
  right: 0;
  z-index: 99;
  width: 100%;
  height: 50px;
  float: left;
  border-top: 1px solid rgba(102, 102, 102, 0.1);
  display: flex;
  background-color: white;
}

.tabbar>.tabbar-item {
  flex: 1;
  text-align: center;
}

.tabbar>.tabbar-item>.icon {
  width: 100%;
  height: auto;
  float: left;
  margin-top: 5px;
}

.tabbar>.tabbar-item>.icon>image {
  width: 28px;
  height: 28px;
}

.tabbar>.tabbar-item>.name {
  width: 100%;
  height: 14px;
  font-size: 10px;
  font-weight: 400;
  color: rgba(102, 102, 102, 1);
  line-height: 10px;
}