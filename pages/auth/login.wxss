
.login-button-box {
  width: 100%;
  height: auto;
  float: left;
  margin-top: 100px;
  box-sizing: border-box;
  padding-left: 15px;
  padding-right: 15px;
}


.login-button-box>.login-button {
  display: block;
  width: 100%;
  height: 36px;
  line-height: 36px;
  float: left;
  background-color: #0b76de;
  color: white;
  text-align: center;
  font-size: 14px;
  border-radius: 3px;
}

.protocol {
  width: 100%;
  height: auto;
  float: left;
  padding-left: 15px;
  padding-right: 15px;
  margin-top: 30px;
  color: #aaa;
  font-size: 12px;
}

.protocol>text {
  color: blue;
  font-weight: 300;
}